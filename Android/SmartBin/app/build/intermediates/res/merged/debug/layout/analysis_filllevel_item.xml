<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:id="@+id/analysisFilllayout">

    <CheckBox
        android:id="@+id/chkAnalysis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </CheckBox>


    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chkTextlayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:textSize="15dp"
            android:textColor="@android:color/white"
            android:id="@+id/analysisLtxtView"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:textSize="15dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"
            android:textColor="@android:color/white"
            android:id="@+id/analysisRtxtView"/>

    </RelativeLayout>

</LinearLayout><!-- From: file:/C:/data/xtra/code_portal/gitcode/Android/SmartBin/app/src/main/res/layout/analysis_filllevel_item.xml -->